rint(v3) \n","    plt.bar(v3.keys(), v3.values())\n","    plt.xlabel('Payoff')\n","    plt.ylabel('Count')\n","    plt.grid(axis = 'y', alpha = 0.75)\n","    plt.show()"],"execution_count":6,"outputs":[]},{"cell_type":"code","metadata":{"colab":{"base_uri":"https://localhost:8080/","height":905},"id":"ZtOzFn6b2tqs","executionInfo":{"status":"ok","timestamp":1631593389275,"user_tz":-420,"elapsed":531,"user":{"displayName":"Quan Nguyen","photoUrl":"https://lh3.googleusercontent.com/a/default-user=s64","userId":"11096386105956491339"}},"outputId":"96f4e639-c54b-4c75-f867-420e402e90f2"},"source":["# Testing Different Sample Sizes\n","sample_sizes = [10, 1000, 100000]\n","for i in range(len(sample_sizes)):\n","    print('Case #', end = '')\n","    print(i + 1, end = '')\n","    print(': Sample Size =', sample_sizes[i])\n","    look_back_option(sample_sizes[i])"],"execution_count":7,"outputs":[{"output_type":"stream","name":"stdout","text":["Case #1: Sample Size = 10\n","Counter({2.0: 4, 3.5: 3, 6.0: 2, 8.0: 1})\n"]},{"output_type":"display_data","data":{"image/png":"\n","text/plain":["<Figure size 432x288 with 1 Axes>"]},"metadata":{"needs_background":"light"}},{"output_type":"stream","name":"stdout","text":["Case #2: Sample Size = 1000\n","Counter({0.0: 369, 2.0: 270, 3.5: 129, 8.0: 129, 6.0: 103})\n"]},{"output_type":"display_data","data":{"image/png":"\n","text/plain":["<Figure size 432x288 with 1 Axes>"]},"metadata":{"needs_background":"light"}},{"output_type":"stream","name":"stdout","text":["Case #3: Sample Size = 100000\n","Counter({0.0: 37630, 2.0: 24920, 6.0: 12580, 3.5: 12491, 8.0: 12379})\n"]},{"output_type":"display_data","data":{"image/png":"\n","text/plain":["<Figure size 432x288 with 1 Axes>"]},"metadata":{"needs_background":"light"}}]},{"cell_type":"markdown","metadata":{"id":"7LMC0azr2xHM"},"source":["End of Simulation."]}]}